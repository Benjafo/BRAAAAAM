{
    "name": "server",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"**/*.ts\"",
        "format:check": "prettier --check \"**/*.ts\"",
        "build": "tsc",
        "start": "node ./dist/server/bin/www.js",
        "dev": "tsx watch ./bin/www.ts",
        "typecheck": "tsc --noEmit",
        "test": "NODE_ENV=test jest --runInBand --coverage --detectOpenHandles"
    },
    "dependencies": {
        "@types/cors": "^2.8.19",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "bcrypt": "^6.0.0",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^17.2.3",
        "drizzle-orm": "^0.44.6",
        "drizzle-zod": "^0.8.3",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "http-proxy-middleware": "^3.0.5",
        "jade": "~1.11.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "~1.9.1",
        "pg": "^8.16.3",
        "prettier": "^3.6.2",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/cookie-parser": "^1.4.9",
        "@types/debug": "^4.1.12",
        "@types/express": "^5.0.3",
        "@types/http-errors": "^2.0.5",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.3.0",
        "@types/pg": "^8.15.5",
        "drizzle-kit": "^0.31.5",
        "eslint": "^9.34.0",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.6",
        "typescript": "^5.9.2"
    }
}
